import pandas as pd

# Load all sheet names
xls = pd.ExcelFile("your_file.xlsx")
sheet_names = xls.sheet_names

# Read all sheets into a list of dataframes (skip 2 rows and first column)
dfs = [pd.read_excel(xls, sheet_name=sheet, skiprows=2).iloc[:, 1:] for sheet in sheet_names]

# Optionally, combine all into one dataframe
combined_df = pd.concat(dfs, ignore_index=True)

# To check individual dataframes: dfs[0], dfs[1], etc.