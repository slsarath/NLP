try:
    array_data = np.array(data_values)
    id_sheet.Range(f"A1:DM1").Value = [col_names]
    id_sheet.Range(f"A2:DM{num_rows + 1}").Value = array_data
except ValueError as e:
    print(f"ValueError encountered: {e}")
    # Handle specific NaT error
    if "NaTType does not support astimezone" in str(e):
        # Convert NaT to None or empty string as a fallback
        array_data = np.array([[None if pd.isna(x) else x for x in row] for row in data_values])
        id_sheet.Range(f"A2:DM{num_rows + 1}").Value = array_data