import pandas as pd

# Sample dataframe
df = pd.DataFrame({'date': ['31/12/2024', '15/01/2025', '10/03/2024']})

# Convert to datetime (assuming format is day/month/year)
df['date'] = pd.to_datetime(df['date'], format='%d/%m/%Y')

# Create new column with 'Dec-2024' format
df['month_year'] = df['date'].dt.strftime('%b-%Y')

print(df)